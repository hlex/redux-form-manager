{
  "name": "redux-form-manager",
  "version": "2.3.0",
  "description": "Redux form manager",
  "main": "dist/index.js",
  "repository": {
    "url": "https:git@github.com:hlex/redux-form-manager.git",
    "type": "git"
  },
  "author": "Mondit Thumniramon (mondit.thum@gmail.com)",
  "license": "MIT",
  "keywords": [
    "redux",
    "form",
    "validation",
    "form validation"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "better-npm-run dev",
    "start": "better-npm-run start",
    "clean": "better-npm-run clean",
    "tester": "better-npm-run test",
    "test": "npm run lint && npm run tester",
    "lint": "eslint ./app/*/**.js",
    "build": "npm run clean && better-npm-run build",
    "build:demo": "better-npm-run clean:demo && better-npm-run build:demo",
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prerelease": "npm version minor && npm run build && git push origin master --tags",
    "release": "npm publish",
    "postrelease": "git commit -m 'fix: release' && git push"
  },
  "betterScripts": {
    "dev": {
      "command": "node ./scripts/devServer.js --progress --hide-modules --config ./configs/webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development",
        "BUILD_ENV": "server"
      }
    },
    "build": {
      "command": "cross-env NODE_ENV=production babel --copy-files --out-dir dist --ignore *.spec.js src"
    },
    "build:demo": {
      "command": "webpack --progress --hide-modules --config ./configs/webpack.config.production.js",
      "env": {
        "NODE_ENV": "production",
        "BUILD_ENV": "client"
      }
    },
    "start": {
      "command": "node ./scripts/start.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "mocha ./test/*.js --compilers js:babel-register --watch"
    },
    "clean": {
      "command": "rimraf dist"
    },
    "clean:demo": {
      "command": "rimraf demo"
    }
  },
  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "better-npm-run": "^0.0.15",
    "cross-env": "^5.0.1",
    "es6-promise": "^4.0.5",
    "express": "^4.14.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "react-dev-utils": "^0.5.2",
    "react-input-forms": "^1.2.5",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0"
  }
}
